	ДанныеXDTO.Вставить("СкладПолучатель", Новый Структура("Значение, ИмяПКО", ДанныеИБ.МестоХраненияПриемник,"Справочник_Склады_Отправка"));
	
	ДобавитьЕдиницуИзмеренияВТаблицуТовары(ДанныеXDTO.Товары);
