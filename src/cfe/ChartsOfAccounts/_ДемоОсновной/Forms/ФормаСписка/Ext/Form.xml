			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ПланСчетов_ДемоОсновной.Ссылка,
	
	ПланСчетов_ДемоОсновной.ПометкаУдаления,
	ПланСчетов_ДемоОсновной.Предопределенный,
	
	ПланСчетов_ДемоОсновной.Родитель,
	ПланСчетов_ДемоОсновной.Код,
	ПланСчетов_ДемоОсновной.Наименование,
	ПланСчетов_ДемоОсновной.Порядок,
	ПланСчетов_ДемоОсновной.Вид,
	ПланСчетов_ДемоОсновной.Забалансовый,
	ПланСчетов_ДемоОсновной.Валютный,
	ПланСчетов_ДемоОсновной.Количественный,
	ПланСчетов_ДемоОсновной.ВидыСубконто,
	ПланСчетов_ДемоОсновной.ИмяПредопределенныхДанных,

	Субконто1.ВидСубконто КАК Субконто1,
	Субконто2.ВидСубконто КАК Субконто2,
	Субконто3.ВидСубконто КАК Субконто3,

	ВЫБОР
		КОГДА ПланСчетов_ДемоОсновной.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
			ТОГДА "А"
		ИНАЧЕ
			ВЫБОР
				КОГДА ПланСчетов_ДемоОсновной.Вид = ЗНАЧЕНИЕ(ВидСчета.Пассивный)
					ТОГДА "П"
				ИНАЧЕ "АП"
			КОНЕЦ
	КОНЕЦ КАК ВидСтрокой,
	
	ВЫБОР 
		КОГДА ПланСчетов_ДемоОсновной.ПометкаУдаления ТОГДА
			ВЫБОР 
				КОГДА ПланСчетов_ДемоОсновной.Предопределенный ТОГДА 3
				ИНАЧЕ 2
			КОНЕЦ
		ИНАЧЕ
			ВЫБОР 
				КОГДА ПланСчетов_ДемоОсновной.Предопределенный ТОГДА 1
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК ИндексПиктограммы
	
ИЗ
	ПланСчетов._ДемоОсновной КАК ПланСчетов_ДемоОсновной
	
ЛЕВОЕ СОЕДИНЕНИЕ
	ПланСчетов._ДемоОсновной.ВидыСубконто КАК Субконто1
ПО
	ПланСчетов_ДемоОсновной.Ссылка = Субконто1.Ссылка И Субконто1.НомерСтроки = 1
	
ЛЕВОЕ СОЕДИНЕНИЕ
	ПланСчетов._ДемоОсновной.ВидыСубконто КАК Субконто2
ПО
	ПланСчетов_ДемоОсновной.Ссылка = Субконто2.Ссылка И Субконто2.НомерСтроки = 2
	
ЛЕВОЕ СОЕДИНЕНИЕ
	ПланСчетов._ДемоОсновной.ВидыСубконто КАК Субконто3
ПО
	ПланСчетов_ДемоОсновной.Ссылка = Субконто3.Ссылка И Субконто3.НомерСтроки = 3
</QueryText>
